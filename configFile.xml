<!--
Overal configuration file
-->

<xml>
	<config>
		<!--Misc-->
		<log4j>DEBUG</log4j><!--Logging level : ERROR,INFO,DEBUG-->
		<csvsplitter>,</csvsplitter>
		<allowedfiles>txt,csv</allowedfiles>
		<sourcedirectory>Files</sourcedirectory>
		<timezone>Europe/Paris</timezone>
		<!--CDR matcher-->
		<starttime>dateTimeConnect</starttime>
		<endtime>dateTimeDisconnect</endtime>
		<callingnumber>callingPartyNumber</callingnumber>
		<callednumber>originalCalledPartyNumber</callednumber>
		<callingname>origDeviceName</callingname>
		<calledname>destDeviceName</calledname>
		<!--CDR Server parameters-->
		<cdrhost>10.0.1.172</cdrhost><!--IP of the CUCM server-->
		<cdrport>8443</cdrport><!--Port to contact the CUCM server using CDR-->
		<cucmversion>105</cucmversion><!--85 or 105--><!--CDR version of the CUCM-->
		<cdrusername>SATAdmin</cdrusername><!--CDR user name to contact the CUCM server-->
		<cdrpassword>CCMCisco92</cdrpassword><!--Password of the CDR user in clear !!!!Warning : keep it secret!!!!-->
	</config>
</xml>